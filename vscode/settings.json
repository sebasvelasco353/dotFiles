{
  "breadcrumbs.enabled": true,
  "breadcrumbs.filePath": "last",
  "editor.glyphMargin": false,
  "editor.autoIndent": "full",
  "editor.codeLens": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "editor.formatOnPaste": false,
  "editor.lineHeight": 25,
  "editor.multiCursorModifier": "alt",
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "editor.autoClosingBrackets": "always",
  "editor.fontFamily": "'JetBrainsMono Nerd Font', 'Cascadia Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "editor.scrollbar.vertical": "auto",
  "editor.overviewRulerBorder": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.formatOnSave": true,
  "editor.guides.indentation": false,
  "indenticator.width": 0.2,
  "indenticator.color.dark": "rgba(255,255,255,0.2)",
  "files.autoSaveDelay": 2500,
  "files.defaultLanguage": "",
  "files.trimTrailingWhitespace": true,
  "testing.followRunningTest": false,
  "testing.openTesting": "neverOpen",
  "terminal.integrated.localEchoStyle": "dim",
  "workbench.sideBar.location": "right",
  "colorize.languages": [
    "javascript",
    "jsx"
  ],
  "window.zoomLevel": -1,
  "prettier.enable": false,
  "editor.lineNumbers": "relative",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.iconTheme": "material-icon-theme",
  "find-it-faster.general.useTerminalInEditor": false
}
